#
# Sprite attribute parsing order.
#
# @STARTSPRITE
# filename
# identifier
# rows
# columns
# @ENDSPRITE
# No comments must exist between @-@. MANDATORY REQUIREMENT.
#
# @STARTPLAYER
# guy.bmp
# guy
# 4
# 3
# @ENDSPRITE
#
# @STARTCURSOR
# cursor.bmp
# cursor
# @ENDCURSOR
#
# @ZOMBIE
# zombie.bmp
# zombie
# 9
# 3
# @
# 
# @TILE
# tile.bmp
# 1
# 1
# @

@ZOMBIESPAWNER
zombiespawner.bmp
zombiespawner
1
1
@

@CURSOR
cursor.bmp
cursor
@

@PLAYER
guy.bmp
player
8
3
@

@PROJECTILE
bullet.bmp
bullet
4
1
@

@ZOMBIE
knifezombie.bmp
zombie
9
3
@

@TILE
floor.bmp
floor
1
1
@
